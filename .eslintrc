{
  "env": {
    "node": true,
    "browser": true
  },
  "extends": [
    "think"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "camelcase": "off"
  }
}